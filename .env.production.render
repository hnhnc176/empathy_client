# Sau khi có Render URL, update file này

# Thay thế URL này bằng URL thật từ Render
VITE_API_BASE_URL=https://empathy-backend-xxxx.onrender.com/api
VITE_SOCKET_URL=https://empathy-backend-xxxx.onrender.com

# Các config khác giữ nguyên
VITE_APP_NAME=Empathy
VITE_APP_ENVIRONMENT=production
VITE_NODE_ENV=production

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_HTTPS=true

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=dbbrslu7j
VITE_CLOUDINARY_UPLOAD_PRESET=empathy_uploads

# API Configuration
VITE_API_TIMEOUT=15000
VITE_API_RETRY_ATTEMPTS=3